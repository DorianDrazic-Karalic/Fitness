<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="272.0" prefWidth="300" spacing="10" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER" prefHeight="26.0" prefWidth="279.0" spacing="20.0" />
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Exercise" />
      <HBox alignment="CENTER_LEFT" spacing="10">
         <children>
            <Label prefHeight="18.0" prefWidth="50.0" text="Name:">
               <HBox.margin>
                  <Insets top="20.0" />
               </HBox.margin></Label>
            <TextField fx:id="exerciseNameField" prefHeight="26.0" prefWidth="200.0">
               <HBox.margin>
                  <Insets top="20.0" />
               </HBox.margin></TextField>
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" spacing="10">
         <children>
            <Label prefWidth="50.0" text="Calories:" />
            <TextField fx:id="exerciseCaloriesField" prefHeight="26.0" prefWidth="200.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" spacing="10" />
      <HBox alignment="CENTER_LEFT" spacing="10" />
      <HBox alignment="CENTER_LEFT" spacing="10" />
      <Button fx:id="addExerciseWindowButton" alignment="CENTER" contentDisplay="CENTER" prefHeight="26.0" prefWidth="278.0" stylesheets="@../resources/values/style.css" text="Add Exercise" />
      <HBox alignment="BASELINE_RIGHT" spacing="10" />
      <HBox alignment="BASELINE_RIGHT" spacing="10" />
      <HBox alignment="BASELINE_RIGHT" spacing="10" />
      <HBox alignment="CENTER_LEFT" spacing="10" />
   </children>
   <padding>
      <Insets bottom="10" left="10" right="10" top="10" />
   </padding>
</VBox>
